{
  "name": "concis-ui",
  "private": true,
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "examples:dev": "cd examples && yarn && yarn dev",
    "test:pkg": "jest ./tests -u",
    "pre-commit": "pretty-quick --staged && npm run eslint && npm run stylelint",
    "eslint": "eslint docs/ packages/concis-react/src --cache --fix --quiet --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint {packages,docs,.dumi/theme}/**/*.less --fix",
    "format": "prettier --config .prettierrc.js --write \"{packages,docs}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && dumi setup"
  },
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
